{
  "name": "sado-kueccha",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "packageManager": "pnpm@10.12.1",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "keywords": [
    "react",
    "typescript",
    "vite",
    "maps",
    "sado"
  ],
  "scripts": {
    "dev": "vite",
    "build": "pnpm type-check && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --flag unstable_config_lookup_from_file",
    "lint:fix": "eslint . --fix --flag unstable_config_lookup_from_file",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "ci": "pnpm type-check && pnpm lint && pnpm test:run",
    "clean": "rimraf dist",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@googlemaps/markerclusterer": "^2.5.3",
    "@react-oauth/google": "^0.12.2",
    "@vis.gl/react-google-maps": "^1.5.2",
    "gapi-script": "^1.2.0",
    "google-spreadsheet": "^4.1.4",
    "googleapis": "^149.0.0",
    "japanese-holidays": "^1.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.11",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.33.0",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix --flag unstable_config_lookup_from_file"
    ]
  },
  "sideEffects": false
}
