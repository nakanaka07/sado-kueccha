{
  "name": "full",
  "description": "全機能有効プロファイル - 開発・デバッグ重視",
  "performance": {
    "bundleSize": {
      "maxChunkSize": 4096000,
      "maxAssetSize": 2048000
    },
    "buildTime": {
      "maxBuildTime": 120
    },
    "runtime": {
      "initialLoadTime": 5000,
      "cacheHitRate": 70
    }
  },
  "features": {
    "enablePWA": true,
    "enableBundleAnalyzer": true,
    "enableDevTools": true,
    "enableSourceMaps": true,
    "enableHotReload": true,
    "enableErrorOverlay": true
  },
  "plugins": {
    "essential": [
      "@vitejs/plugin-react-swc",
      "vite-plugin-pwa",
      "rollup-plugin-visualizer",
      "@vitejs/plugin-legacy"
    ]
  },
  "build": {
    "minify": "terser",
    "treeshake": "recommended",
    "splitChunks": true,
    "optimization": "full"
  },
  "debugging": {
    "enableProfiling": true,
    "enableLogging": true,
    "verboseOutput": true
  }
}
