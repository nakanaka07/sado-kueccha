{
  "name": "test",
  "description": "テスト環境設定",
  "vite": {
    "server": {
      "port": 5174,
      "host": "localhost",
      "https": false,
      "hmr": false,
      "open": false
    },
    "build": {
      "sourcemap": true,
      "minify": false,
      "reportCompressedSize": false
    }
  },
  "api": {
    "baseUrl": "http://localhost:3001",
    "timeout": 5000,
    "retries": 0
  },
  "features": {
    "devTools": false,
    "bundleAnalyzer": false,
    "pwa": false,
    "errorOverlay": false,
    "hotReload": false
  },
  "logging": {
    "level": "warn",
    "enableConsole": true,
    "enableNetwork": false
  },
  "performance": {
    "optimizationLevel": "minimal",
    "enableProfiling": false
  },
  "testing": {
    "coverage": {
      "threshold": 80,
      "reportFormats": ["text", "html", "lcov"]
    },
    "timeout": 10000,
    "parallel": true,
    "mockApi": true
  }
}
