{
    "extends": "./tsconfig.app.json",
    "compilerOptions": {
        "types": [
            "node",
            "google.maps",
            "vitest/globals",
            "@testing-library/jest-dom"
        ],
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "strictNullChecks": true,
        "incremental": true,
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.test.tsbuildinfo"
    },
    "include": [
        "src/**/*.test.ts",
        "src/**/*.test.tsx",
        "src/**/*.spec.ts",
        "src/**/*.spec.tsx",
        "src/setupTests.ts"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}