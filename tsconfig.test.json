{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "types": ["google.maps", "@testing-library/jest-dom", "vitest/globals"],
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.test.tsbuildinfo"
  },
  "include": [
    "src/**/*.test.ts",
    "src/**/*.test.tsx",
    "src/**/*.spec.ts",
    "src/**/*.spec.tsx",
    "src/setupTests.ts"
  ]
}
